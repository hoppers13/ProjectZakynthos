@using ProjectZakynthos.Web.Areas.stamps.Models
@model 	ManageWishlistViewModel
@{
    ViewBag.Title = "Manage";
}

<h2>Manage wishlist</h2>

@using (Html.BeginForm("Manage"))
{
	<table class="table table-striped">
		<thead>
			<tr>
                <th>Catalogue Number</th>
                <th>Area</th>                
				<th>Year</th>
				<th>Description</th>
				<th>Condition</th>
			</tr>
		</thead>
        <tbody>
            @Html.EditorFor(x => x.Items)
        </tbody>		
	</table>
	<input type="submit" class="btn btn-info btn-lg" value="Save" />
}